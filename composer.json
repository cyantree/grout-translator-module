{
    "name": "cyantree/grout-translator-module",
    "version": "1.0",
    "type": "grout-module",
    "description": "module for translations",
    "keywords": ["grout", "i18n", "translation"],
    "homepage": "https://github.com/cyantree/grout-translator-module",
    "license": "LGPL-3.0",
    "authors": [
        {"name": "Christoph Schreiber", "email": "mail@cyantree.com"}
    ],
    "autoload": {
        "psr-4": {
            "Grout\\Cyantree\\TranslatorModule\\": ["", "source/"]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        }
    ],
    "require": {
        "cyantree/grout": "1.*",
        "zendframework/zend-i18n": "2.2.*",
        "zendframework/zend-cache": "2.2.*",
        "zendframework/zend-serializer": "2.2.*"
    },
    "extra": {
        "grout-module-name": "Cyantree/TranslatorModule"
    }
}